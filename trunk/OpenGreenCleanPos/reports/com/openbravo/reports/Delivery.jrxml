	<queryString>
		<![CDATA[SELECT CUSTOMERS.SEARCHKEY, TICKETS.TICKETID, TICKETS.DATERETURN, PRODUCTS.REFERENCE, PRODUCTS.NAME, SUM(TICKETLINES.UNITS) AS UNITS, SUM(TICKETLINES.UNITS * TICKETLINES.PRICE) AS TOTAL
FROM RECEIPTS, TICKETS LEFT OUTER JOIN CUSTOMERS ON CUSTOMERS.ID = TICKETS.CUSTOMER, TICKETLINES LEFT OUTER JOIN PRODUCTS ON TICKETLINES.PRODUCT = PRODUCTS.ID
WHERE RECEIPTS.ID = TICKETS.ID AND TICKETS.ID = TICKETLINES.TICKET
GROUP BY TICKETS.TICKETID,CUSTOMERS.SEARCHKEY, PRODUCTS.REFERENCE, PRODUCTS.NAME
ORDER BY TICKETS.TICKETID,CUSTOMERS.SEARCHKEY, PRODUCTS.NAME]]>
